<?xml version="1.0" encoding="UTF-8"?>

<!--
Spécification des batchs
-->

<CATALOG>
    <BATCH>
        <CODE>07J</CODE>
        <NAME>Batch num 7 - extraction 7</NAME>
        <DESCRIPTION>USA</DESCRIPTION>
        <OUTPUT>Fichier xls</OUTPUT>
        <INPUT>
            <PARAM>
                <!-- référence Mora du produit -->
                <NAME>refMora</NAME>
                <DEFAULTVALUE></DEFAULTVALUE>
                <LABEL>Référence Mora</LABEL>
            </PARAM>
            <PARAM>
                <!-- Début de la période -->
                <NAME>date1</NAME>
                <DEFAULTVALUE></DEFAULTVALUE>
                <LABEL>Début de la période</LABEL>
            </PARAM>
            <PARAM>
                <!-- Fin de la période -->
                <NAME>date2</NAME>
                <DEFAULTVALUE></DEFAULTVALUE>
                <LABEL>Fin de la période</LABEL>
            </PARAM>
        </INPUT>
    </BATCH>

    <BATCH>
        <CODE>08M</CODE>
        <NAME>Batch num 8 - extraction 8</NAME>
        <DESCRIPTION>USA</DESCRIPTION>
        <OUTPUT>Fichier xls</OUTPUT>
        <INPUT>
            <PARAM>
                <!-- code Client -->
                <NAME>codeClient</NAME>
                <DEFAULTVALUE></DEFAULTVALUE>
                <LABEL>Code client</LABEL>
            </PARAM>
        </INPUT>
    </BATCH>


</CATALOG>
